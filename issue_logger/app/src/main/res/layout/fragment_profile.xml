<?xml version="1.0" encoding="utf-8"?>

<layout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto">

    <data />
<androidx.constraintlayout.widget.ConstraintLayout
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="#EEEEEE"
    tools:context=".fragments.ProfileFragment">

    <androidx.constraintlayout.widget.ConstraintLayout
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:background="@color/white"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintTop_toTopOf="parent">


    <ImageView
        android:id="@+id/profileIcon"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:src="@drawable/mdi_user_outline"
        app:layout_constraintTop_toTopOf="parent"
        android:layout_marginTop="29dp"
        android:layout_marginStart="23dp"
        app:layout_constraintStart_toStartOf="parent"/>

        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="@string/my_profile"
            android:gravity="center"
            android:lineSpacingExtra="8sp"
            android:translationY="-3.88sp"
            android:fontFamily="@font/jost"
            android:textSize="16sp"
            android:textColor="#434343"
            android:letterSpacing="0.01"
            android:layout_marginTop="33dp"
            android:layout_marginStart="15dp"
            app:layout_constraintTop_toTopOf="parent"
            app:layout_constraintStart_toEndOf="@+id/profileIcon"/>

        <ImageView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:src="@drawable/chevron__13pt___iphone_"
            app:layout_constraintTop_toTopOf="parent"
            app:layout_constraintEnd_toEndOf="parent"
            android:layout_marginTop="29dp"
            android:layout_marginEnd="19dp"/>
        
<ImageView
    android:id="@+id/vector1"
    android:layout_width="match_parent"
    android:layout_height="0dp"
    android:background="#EEEEEE"
    android:src="@drawable/vector_242"
    app:layout_constraintTop_toBottomOf="@+id/profileIcon"
    app:layout_constraintStart_toStartOf="parent"
    app:layout_constraintEnd_toEndOf="parent"
    android:layout_marginStart="17dp"
    android:layout_marginTop="30dp"
    android:layout_marginEnd="18dp"/>


        <ImageView
            android:id="@+id/notificationIcon"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:src="@drawable/pajamas_notifications"
            app:layout_constraintTop_toBottomOf="@id/vector2"
            android:layout_marginTop="15dp"
            android:layout_marginStart="23dp"
            app:layout_constraintStart_toStartOf="parent"/>

        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="@string/notificationsProfile"
            android:gravity="center"
            android:lineSpacingExtra="8sp"
            android:translationY="-3.88sp"
            android:fontFamily="@font/jost"
            android:textSize="16sp"
            android:textColor="#434343"
            android:letterSpacing="0.01"
            android:layout_marginTop="16dp"
            android:layout_marginStart="15dp"
            app:layout_constraintTop_toBottomOf="@id/vector2"
            app:layout_constraintStart_toEndOf="@+id/notificationIcon"/>

        <ImageView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:src="@drawable/chevron__13pt___iphone_"
            app:layout_constraintTop_toBottomOf="@id/vector2"
            app:layout_constraintEnd_toEndOf="parent"
            android:layout_marginTop="14dp"
            android:layout_marginEnd="19dp"/>

        <ImageView
            android:id="@+id/vector2"
            android:layout_width="match_parent"
            android:layout_height="0dp"
            android:background="#EEEEEE"
            android:src="@drawable/vector_242"
            app:layout_constraintTop_toBottomOf="@+id/privacyIcon"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintEnd_toEndOf="parent"
            android:layout_marginStart="17dp"
            android:layout_marginTop="30dp"
            android:layout_marginEnd="18dp"/>


        <ImageView
            android:id="@+id/privacyIcon"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:src="@drawable/privacy_icon"
            app:layout_constraintTop_toBottomOf="@id/vector1"
            android:layout_marginTop="15dp"
            android:layout_marginStart="23dp"
            app:layout_constraintStart_toStartOf="parent"/>

        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="@string/privacy_policy"
            android:gravity="center"
            android:lineSpacingExtra="8sp"
            android:translationY="-3.88sp"
            android:fontFamily="@font/jost"
            android:textSize="16sp"
            android:textColor="#434343"
            android:letterSpacing="0.01"
            android:layout_marginTop="16dp"
            android:layout_marginStart="15dp"
            app:layout_constraintTop_toBottomOf="@id/vector1"
            app:layout_constraintStart_toEndOf="@+id/profileIcon"/>

        <ImageView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:src="@drawable/chevron__13pt___iphone_"
            app:layout_constraintTop_toBottomOf="@id/vector1"
            app:layout_constraintEnd_toEndOf="parent"
            android:layout_marginTop="14dp"
            android:layout_marginEnd="19dp"/>

        <ImageView
            android:id="@+id/vector3"
            android:layout_width="match_parent"
            android:layout_height="0dp"
            android:background="#EEEEEE"
            android:src="@drawable/vector_242"
            app:layout_constraintTop_toBottomOf="@+id/notificationIcon"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintEnd_toEndOf="parent"
            android:layout_marginStart="17dp"
            android:layout_marginTop="30dp"
            android:layout_marginEnd="18dp"/>





        <ImageView
            android:id="@+id/helpIcon"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:src="@drawable/material_symbols_help_outline"
            app:layout_constraintTop_toBottomOf="@id/vector3"
            android:layout_marginTop="15dp"
            android:layout_marginStart="23dp"
            app:layout_constraintStart_toStartOf="parent"/>

        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="@string/help"
            android:gravity="center"
            android:lineSpacingExtra="8sp"
            android:translationY="-3.88sp"
            android:fontFamily="@font/jost"
            android:textSize="16sp"
            android:textColor="#434343"
            android:letterSpacing="0.01"
            android:layout_marginTop="16dp"
            android:layout_marginStart="15dp"
            app:layout_constraintTop_toBottomOf="@id/vector3"
            app:layout_constraintStart_toEndOf="@+id/notificationIcon"/>


        <ImageView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:src="@drawable/chevron__13pt___iphone_"
            app:layout_constraintTop_toBottomOf="@id/vector3"
            app:layout_constraintEnd_toEndOf="parent"
            android:layout_marginTop="14dp"
            android:layout_marginEnd="19dp"/>

        <ImageView
            android:id="@+id/vector4"
            android:layout_width="match_parent"
            android:layout_height="0dp"
            android:background="#EEEEEE"
            android:src="@drawable/vector_242"
            app:layout_constraintTop_toBottomOf="@+id/helpIcon"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintEnd_toEndOf="parent"
            android:layout_marginStart="17dp"
            android:layout_marginTop="30dp"
            android:layout_marginEnd="18dp"/>






        <ImageView
            android:id="@+id/faqIcon"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:src="@drawable/wpf_faq"
            app:layout_constraintTop_toBottomOf="@id/vector4"
            android:layout_marginTop="15dp"
            android:layout_marginStart="23dp"
            app:layout_constraintStart_toStartOf="parent"/>

        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="@string/faq_s"
            android:gravity="center"
            android:lineSpacingExtra="8sp"
            android:translationY="-3.88sp"
            android:fontFamily="@font/jost"
            android:textSize="16sp"
            android:textColor="#434343"
            android:letterSpacing="0.01"
            android:layout_marginTop="16dp"
            android:layout_marginStart="15dp"
            app:layout_constraintTop_toBottomOf="@id/vector4"
            app:layout_constraintStart_toEndOf="@+id/faqIcon"/>


        <ImageView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:src="@drawable/chevron__13pt___iphone_"
            app:layout_constraintTop_toBottomOf="@id/vector4"
            app:layout_constraintEnd_toEndOf="parent"
            android:layout_marginTop="14dp"
            android:layout_marginEnd="19dp"/>

        <ImageView
            android:id="@+id/vector5"
            android:layout_width="match_parent"
            android:layout_height="0dp"
            android:background="#EEEEEE"
            android:src="@drawable/vector_242"
            app:layout_constraintTop_toBottomOf="@+id/faqIcon"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintEnd_toEndOf="parent"
            android:layout_marginStart="17dp"
            android:layout_marginTop="30dp"
            android:layout_marginEnd="18dp"/>







        <ImageView
            android:id="@+id/logoutIcon"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:src="@drawable/logout_1_"
            app:layout_constraintTop_toBottomOf="@id/vector5"
            android:layout_marginTop="15dp"
            android:layout_marginStart="23dp"
            app:layout_constraintStart_toStartOf="parent"/>

        <TextView
            android:id="@+id/logout"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="@string/logout"
            android:gravity="center"
            android:lineSpacingExtra="8sp"
            android:translationY="-3.88sp"
            android:fontFamily="@font/jost"
            android:textSize="16sp"
            android:textColor="#434343"
            android:letterSpacing="0.01"
            android:layout_marginTop="16dp"
            android:layout_marginStart="15dp"
            app:layout_constraintTop_toBottomOf="@id/vector5"
            app:layout_constraintStart_toEndOf="@+id/logoutIcon"/>


        <ImageView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:src="@drawable/chevron__13pt___iphone_"
            app:layout_constraintTop_toBottomOf="@id/vector5"
            app:layout_constraintEnd_toEndOf="parent"
            android:layout_marginTop="14dp"
            android:layout_marginEnd="19dp"/>

        <ImageView
            android:id="@+id/vector6"
            android:layout_width="match_parent"
            android:layout_height="0dp"
            android:background="#EEEEEE"
            android:src="@drawable/vector_242"
            app:layout_constraintTop_toBottomOf="@+id/logoutIcon"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintEnd_toEndOf="parent"
            android:layout_marginStart="17dp"
            android:layout_marginTop="30dp"
            android:layout_marginEnd="18dp"/>











    </androidx.constraintlayout.widget.ConstraintLayout>




</androidx.constraintlayout.widget.ConstraintLayout>
</layout>